{
    "properties": {
        "title": "${apptitle}"
    },
    "load": {
        "bundleLocations": [
            "localbundles",
            "bundles"
        ],
        "allowedBundles": [
            "agssearch",
            "banner",
            "basemaptoggler",
            "languagetoggler",
            "locateme",
            "locator-store",
            "map-init",
            "map-preload-2D",
            "mapnavigation",
            "measurement-2d",
            "notifier",
            "parameter-url",
            "popups-default",
            "printing",
            "result-ui",
            "scalebar",
            "search-ui",
            "selection-ui",
            "sketching",
            "splashscreen",
            "system",
            "template-seasons",
            "templatelayout",
            "theme-autumn",
            "toc",
            "toolrules",
            "toolset",
            "dn_coordinatesearch",
            "dn_imprintprivacy",
            "dn_weathervisualization"
        ],
        "i18n": [
            "bundle"
        ]
    },
    "bundles": {
        "dn_coordinatesearch": {
            "DecimalCoordinateSearchStore": {
                "componentEnabled": true,
                "showExample": false
            },
            "UTMCoordinateSearchStore": {
                "componentEnabled": true,
                "showExample": false,
                "conversionMode": "latitude-band-indicators"
            },
            "GKCoordinateSearchStore": {
                "componentEnabled": true,
                "showExample": false
            },
            "PseudoMercatorCoordinateSearchStore": {
                "componentEnabled": true,
                "showExample": false
            }
        },
        "agssearch": {
            "AutoStoreRegistration": {
                "componentEnabled": true
            }
        },
        "banner": {
            "BannerWidget": {
                "label": "Developer Network",
                "image": "resource('${app}:/assets/logo.svg')",
                "imageWidth": 150,
                "imagePosition": "above",
                "link": "http://conterra.github.io"
            }
        },
        "basemaptoggler": {
            "Config": {
                "itemStyles": {
                    "gray": "icon-thumbnail",
                    "streets": "icon-thumbnail",
                    "satellite": "icon-thumbnail",
                    "hybrid": "icon-thumbnail"
                }
            }
        },
        "map-init": {
            "Config": {
                "basemaps": [
                    {
                        "id": "gray",
                        "basemap": "gray-vector",
                        "title": "${map.basemaps.gray}",
                        "thumbnailUrl": "resource('${app}:/assets/gray.png')",
                        "selected": true
                    },
                    {
                        "id": "streets",
                        "basemap": "streets-vector",
                        "title": "${map.basemaps.streets}",
                        "thumbnailUrl": "resource('${app}:/assets/streets.png')"
                    },
                    {
                        "id": "satellite",
                        "basemap": "satellite",
                        "title": "${map.basemaps.topo}",
                        "thumbnailUrl": "resource('${app}:/assets/topo.png')"
                    },
                    {
                        "id": "hybrid",
                        "basemap": "hybrid",
                        "title": "${map.basemaps.hybrid}",
                        "thumbnailUrl": "resource('${app}:/assets/hybrid.png')"
                    }
                ],
                "map": {
                    "layers": [
                        {
                            "id": "pois",
                            "title": "${layers.pois}",
